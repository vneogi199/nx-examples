{
  "name": "shared-jsxify",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/jsxify/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "clean": true,
        "generateLockfile": true,
        "updateBuildableProjectDepsInPackageJson": true,
        "main": "libs/shared/jsxify/src/index.ts",
        "outputPath": "dist-example/jsxify",
        "tsConfig": "libs/shared/jsxify/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/jsxify/src/**/*.ts",
          "libs/shared/jsxify/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["scope:shared", "type:types"]
}
